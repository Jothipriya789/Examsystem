@model IEnumerable<Examsystem.Models.Report>

@{
    ViewBag.Title = "Results";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br/><br/><br/>
<h2>Results</h2>
@if (ViewBag.not != null)
{
    <h2><center>@ViewBag.not</center></h2>
    <div align="center">
        <a href="@Url.Action("Getdetailsstudents", new {  } )" class="btn btn-primary">ok</a>
    </div>

}
@if (ViewBag.not == null)
{
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.result_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.result_status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.result_score)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.user_id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.exam_id)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.result_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.result_status)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.result_score)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.user_id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.exam_id)
                </td>

            </tr>
        }

    </table>
    <div align="center">
        <a href="@Url.Action("Getdetailsstudents", new {  } )" class="btn btn-primary">ok</a>
    </div>
}